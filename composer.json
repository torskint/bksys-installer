{
    "name": "torskint/laravel-banking-package",
    "description": "Installation automatique.",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1.0",
        "laravel/framework": "^8.0"
    },
    "scripts": {
        "post-install-cmd": [
            "@php artisan key:generate"
        ],
        "create-project": [
            "composer create-project --prefer-dist laravel/laravel temp-project",
            "mv temp-project/* temp-project/.* .",
            "mv temp-project/.gitignore .",
            "rm -rf temp-project"
        ]
    }
}
